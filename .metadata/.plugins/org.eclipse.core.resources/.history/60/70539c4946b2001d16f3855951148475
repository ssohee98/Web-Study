<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	//JavaScript 자료형
	
	
	//숫자형(number)
	let intNum = 10;
	let floatNum = 3.14;
	
	console.log(typeof intNum);
	console.log(typeof floatNum);
	
	
	//문자형(string)
	let sinS = 'single';
	let doubleS = "double";
	
	console.log(typeof sinS);
	console.log(typeof doubleS);
	
	
	//논리형(boolean)
	let boolVar = true;
	console.log(typeof boolVar);
	
	
	//undefined 
	let emptyVar;
	console.log(typeof emptyVar);
	
	
	let nullVar = null;		//null 타입은 object
	console.log(typeof nullVar);
	
	
	//함수(function)
	let fun = function() {
		
	};
	console.log(typeof fun);
	

	//형변환
	intNum = '문자열';		// intNum이 string문자열로 형변환된다.
	console.log(typeof intNum);
	
	
	//문자열 => 정수형 : eval(), Number(), parseInt():숫자.문자형이 섞여있어도 숫자만 변환해준다.
	let num = '100안녕';
	num = parseInt(num);
	let num2 = num + 100;
	console.log(num2);
	console.log(typeof num2);
	
	let num3 = '400';
	
	//데이터 타입이 같은지 확인하려면 등호3개 : num2-number / num3-string
	if(num2 === num3) {
		console.log('같다');
	} else {
		console.log('다르다');
	}
	
	
</script>
</head>
<body>

</body>
</html>